{
  "$schema": "http://api.contactlab.it/hub#",
  "type": "object",
  "description": "Customer compiled a form on the website/app/convention",
  "properties": {
    "customerId": {
      "type": "string",
      "pattern": "^.+$"
    },
    "bringBackProperties": {
      "$ref": "resource:/public/schema/enums.json#/definitions/EventBringBackProperties"
    },
    "type": {
      "$ref": "resource:/public/schema/enums.json#/definitions/EventType"
    },
    "context": {
      "$ref": "resource:/public/schema/enums.json#/definitions/EventContext"
    },
    "date": {
      "type": "string",
      "format": "date-time"
    },
    "contextInfo": {
      "$ref": "resource:/public/schema/enums.json#/definitions/EventContextInfo"
    },
    "properties": {
      "type": "object",
      "properties": {
        "formName": {
          "type": "string",
          "description": "The name to identify the form"
        },
        "formId": {
          "type": "string",
          "description": "The id to identify the form"
        },
        "data": {
          "type": "object",
          "description": "The entire form data"
        },
        "extraProperties": {
          "type": "object",
          "description": "Extra pieces of information you can tie to events you track"
        }
      }
    }
  },
  "required": [
    "type",
    "context"
  ],
  "additionalProperties": false
}